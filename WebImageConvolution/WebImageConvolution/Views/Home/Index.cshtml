@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h4>Image Upload</h4>
    <form asp-controller="Home" asp-action="SingleFile"
          enctype="multipart/form-data" method="post">
        <input type="file" name="file" />
        <p>
            <button type="submit">Submit</button>
        </p>
    </form>
</div>
